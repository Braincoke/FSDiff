<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressIndicator?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="500.0" prefWidth="700.0"  xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="gui.loaders.LoadingFSCXController">
    <BorderPane
            AnchorPane.topAnchor="0"
            AnchorPane.rightAnchor="0"
            AnchorPane.leftAnchor="0"
            AnchorPane.bottomAnchor="0">
       <bottom>
              <HBox alignment="BOTTOM_RIGHT" prefHeight="40.0" prefWidth="200.0" BorderPane.alignment="CENTER">
                  <children>
                      <Button fx:id="cancel" mnemonicParsing="false" onAction="#cancel" text="Cancel" />
                  </children>
              </HBox>
       </bottom>
       <center>
              <VBox alignment="CENTER" prefHeight="300.0" prefWidth="100.0" spacing="20.0" BorderPane.alignment="CENTER">
                  <children>
                      <ProgressIndicator prefHeight="250.0" prefWidth="250.0" fx:id="progressIndicator" />
                <Label text="Loading" fx:id="loadingLabel" />
                  </children>
              </VBox>
       </center>
       <top>
          <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER">
             <children>
                      <Label text="Loading">
                          <font>
                              <Font size="36.0" />
                          </font>
                      </Label>
             </children>
          </HBox>
       </top>
       <padding>
          <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
       </padding>
    </BorderPane>
</AnchorPane>
