<?xml version="1.0" encoding="UTF-8"?>

<!-- This is the main Window of FSDiff where the user can compare 2 file systems (or logical directories) -->

<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" stylesheets="@ComparisonWindow.css" xmlns="http://javafx.com/javafx/8" fx:controller="gui.comparison.ComparisonWindowController">
   <children>
      <VBox prefHeight="800.0" prefWidth="1000.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <fx:include fx:id="menuBar" source="../MenuBar.fxml" />
            <fx:include fx:id="toolbar" source="Toolbar.fxml" />
            <fx:include fx:id="breadcrumbs" source="Breadcrumbs.fxml" />
            <SplitPane id="mainPane" fx:id="splitPane" dividerPositions="0.3" prefHeight="600.0" prefWidth="800.0" VBox.vgrow="ALWAYS">
               <items>
                     <fx:include fx:id="leftMenu" source="LeftMenu.fxml" />
                     <fx:include fx:id="dataPane" source="DataPane.fxml" />
               </items>
            </SplitPane>
            <fx:include fx:id="bottomPane" source="BottomPane.fxml" />
         </children>
      </VBox>
   </children>
</AnchorPane>
